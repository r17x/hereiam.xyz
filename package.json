{
  "name": "hereiam.xyz",
  "author": "ri7nz <ri7nz.labs@gmail.com>",
  "version": "1.0.0",
  "homepage": "https://hereiam.xyz",
  "private": true,
  "dependencies": {
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-scripts": "2.1.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:logo": "./buildlogo.js",
    "lh": "lighthouse-ci",
    "test": "cross-env NODE_PATH=src/ react-scripts test --env=jsdom --coverage",
    "test:lh": "yarn lh --performance=95 --seo=100 --pwa=100 --accessibility=100 --best-practices=100",
    "eject": "react-scripts eject",
    "build:storybook": "build-storybook -s public",
    "storybook": "start-storybook -p 9009 -s public",
    "deploy": "yarn build && gh-pages -d build",
    "deploy:travis": "gh-pages-with-token -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "axios": "0.18.0",
    "eslint-config-prettier": "3.6.0",
    "eslint-plugin-jest": "22.4.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "gh-pages": "2.2.0",
    "gh-pages-with-token": "1.0.0",
    "husky": "1.3.1",
    "lighthouse-ci": "1.6.0",
    "lint-staged": "8.1.4",
    "node-sass": "4.11.0",
    "prettier": "1.16.4",
    "sharp": "0.21.3"
  }
}
